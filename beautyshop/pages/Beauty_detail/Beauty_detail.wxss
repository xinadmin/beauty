page {
  background-color: #FBF7F8;
}
.top_bg {
  height: 430rpx;
  width: 100%;
  background-color: #2A2A2A;
}
.mid_bg {
  width: 100%;
  height: 460rpx;
  background-color: #fff;
  position: relative;
  
}
.mid_bg .mid_bg_top {
  width: 90%;
  margin: 0 auto;
  height: 550rpx;
  text-align: center;
  background-color: #fff;
  position: absolute;
  left: 5%;
  border-radius: 5px;
  top: -90rpx;
}
.mid_bg .mid_bg_top .mid_bg_top_img {
  width: 180rpx;
  height: 180rpx;
  border-radius:50%;
  position: absolute;
  left: 50%;
  margin-left: -90rpx;
  top: -90rpx; 
  background-color: #B5B5B5;
}
.mid_bg .mid_bg_top .mid_bg_top_img  image {
  width: 160rpx;
  height: 160rpx;
  margin: 10rpx;
  border-radius:50%;
}
.mid_bg_top_meaasge {
  width: 100%;
  padding-top: 90rpx;
}
.mid_bg_top_meaasge .mid_bg_top_meaasge_name {
  font-weight: 600;
  font-size: 39rpx;
}
.mid_bg_top_meaasge_logo {
  width: 80%;
  margin: 0 auto;
  text-align: center;
  height: 45rpx;
  line-height: 45rpx;

}
.mid_bg_top_meaasge_logo image {
  width: 45rpx;
  height: 45rpx;
  display: inline-block;

}
 .mid_bg_top_meaasge_fen {
  display:inline-block; 
  color: #888888;
  font-size: 26rpx;
}
.mid_bg_top_meaasge_jieshao {
  width: 100%;
  text-align: center;
  color: #808080;
  font-size: 28rpx;
}
.mid_bg_bottom {
  width: 100%;
  margin-top: 100rpx;
  height: 160rpx;
  box-sizing: border-box;
  padding-left: 66rpx;
  background: url(http://yiyun.zhonyou.cn/webapp_img/dingwei.png) no-repeat left 6rpx;
  background-size: 44rpx;
  position: relative; 

}
.mid_bg_bottom .mid_bg_bottom_address {
    font-size: 26rpx;
    width: 80%; 
    max-height: 110rpx;
    min-height: 55rpx;
    line-height: 55rpx;
    text-align: left;
    color: #232323;
}
.mid_bg_bottom .mid_bg_bottom_min {
    font-size: 26rpx;
    width: 80%; 
    height: 65rpx;
    line-height: 65rpx;
    text-align: left;
    color: #ACACAC;
}

.mid_bg_bottom image {
    width: 80rpx;
    height: 80rpx; 
    position: absolute;
    right: 23rpx;
    top: 25rpx;
}

