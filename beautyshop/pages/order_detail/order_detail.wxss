page {
  background-color: #FBF7F8;
}
.content_div_all {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

#header {
  line-height: 200rpx;
  height: 200rpx;
  width: 100%;
  position: relative;
  background:url(http://yiyun.zhonyou.cn/sprogram_img/order_bg.jpg) repeat-x center center;
  background-size: 100% 100%ï¼›
}

#header s {
  display: inline-block;
  margin: 0 15rpx 0 19rpx;
  font-size: 31rpx;
  color: #fff;
}

#header .img_div {
  height: 200rpx;
  width: 200rpx;
  margin-right: 20rpx;
}

#header .img_div image {
  width: 180rpx;
  height: 140rpx;
  margin-top: 40rpx;
}
.address_message {
  width: 100%;
  height: 146rpx;
  background-color: #fff;
  padding: 30rpx 0; 
}
.address_message image {
  float: left;
  height: 66rpx;
  width:66rpx;
  margin: 40rpx 30rpx;

}
.address_message .address_message_all {
  float: left;
  width: 600rpx;
  color: #383838;
  padding-top: 30rpx;

}
.address_message .address_message_all .address_message_name {
  width: 100%;
  height:60rpx;
  line-height: 60rpx;
  font-size: 29rpx;

}
.address_message .address_message_all .address_message_name .name {
  display: inline-block;
  margin-right: 15rpx;
  }
  
.address_message .address_message_all .address_message_name .tel {
  display: inline-block;
  float: right;
  color: #A6A6A6;
  }
.address_message_address {
     width: 100%;
    height: 90rpx;
    line-height: 45rpx;
     font-size: 28rpx;
  }
.produce {
  width: 100%;
  box-sizing: border-box;
  padding: 0 3% 0;
  margin-top: 0.5rem;
  background: #fff;
}

.produce .produce_shop {
  overflow: hidden;
  width: 100%;
}

.produce .produce_shop s {
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
}

.produce_message {
  width: 100%;
  max-height: 5.8rem;
  overflow: hidden;
}

.produce_message .logo_div {
  float: left;
  width: 180rpx;
  height: 220rpx;
}

.produce_message .logo_div image {
  display: inline-block;
  width: 160rpx;
  height: 170rpx;
}

.produce_message .procuce_message_content {
  float: left;
  width: 510rpx;
  height: 230rpx;
  overflow: hidden;
}

.produce_message .procuce_message_content .procuce_message_content_right {
  width: 100%;
}

.produce_message .procuce_message_content .procuce_message_content_right   .produce_name {
  max-height: 86rpx;
  min-height: 43rpx;
  line-height: 43rpx;
  box-sizing: border-box;
  padding: 6rpx;
  font-size: 28rpx;
  letter-spacing: 0.2px;
  overflow-y: hidden;
  display: -webkit-box;
  color: #333;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.produce_message .procuce_message_content .procuce_message_content_right .guige_all {
  padding: 2% 0;
  color: #333;
  font-size: 25rpx;
}

.produce_message .procuce_message_content .procuce_message_content_right .guige_all em {
  color: #ddd;
  font-size: 25rpx;
}

.produce_message .procuce_message_content .procuce_message_content_right .guige_all s {
  font-size: 26rpx;
}

.produce_message .procuce_message_content .procuce_message_content_right .guige_all .price {
  padding: 2% 0;
  font-size:27rpx;
  color: #f3f3f3;
}

#price {
  width: 100%;
  overflow: hidden;
  background: #fff;
  padding-bottom: 60rpx;
  margin-bottom: 0.5rem;
}

#price view {
  height:65rpx;
  line-height: 65rpx;
  padding: 0 0.6rem;
  color: #383838;
font-size: 31rpx;
}

#price view.jiacu {
  height:45rpx;
