
.hade-a {
  background-color: #fff;
}
#produce_all .produce_title {
color: #232323;
font-weight: 500;
 padding-left: 5%;


}
#produce_all .produce_new {
  display: inline-block;
  background-color: #E98F9B;
color: #fff;
border-radius: 15px;
font-weight: 500;
 margin:3% 5% 4%;
 height: 60rpx;
 line-height: 60rpx;
padding: 0 15rpx;
font-size: 28rpx;

}
.produce_list_litem {
 width: 94%;
 margin: 0 auto;
 overflow: hidden;
}
.produce_list_litem .procuce_message {
  float: left;
 width: 49%;
 box-sizing: border-box;
 margin-bottom:2%;
 overflow: hidden;
 border: solid 1px  #E5E5E5;
}
.produce_list_litem .procuce_message:nth-child(odd) {
 margin-right:2%;
}
.produce_list_litem .procuce_message image {
 width: 100%;
 height: 300rpx;

}
.produce_list_litem .procuce_message .produce_message {
   width: 100%;
   box-sizing: border-box;
   padding: 2% 1.5%;
}
.produce_list_litem .procuce_message .produce_message  .produce_message_name {
color: #111111;
font-size: 31rpx;
height: 66rpx;
line-height: 66rpx;
font-weight: 500;
overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.produce_list_litem .procuce_message .produce_message  .produce_name {
color: #3E3E3E;
font-size: 29rpx;
height: 66rpx;
line-height: 66rpx;
font-weight: 500;
overflow: hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.produce_list_litem .procuce_message .produce_message  .produce_name text {
float: right;
}
.produce_list_litem .procuce_message .produce_message  .produce_price {
font-size: 35rpx;
height: 66rpx;
line-height: 66rpx;
font-weight: 500;
color: #E88F9B;
}
.produce_list_litem .procuce_message .produce_message  .produce_price text {
text-decoration: line-through;
color: #686868;
font-size: 25rpx;
}
